{
    "name": "sonsofphp/feature-toggle",
    "type": "library",
    "description": "Provides feature toggles in PHP",
    "keywords": [
        "feature toggle",
        "flag",
        "toggle",
        "bit",
        "switch",
        "flipper",
        "flip"
    ],
    "homepage": "https://github.com/SonsOfPHP/feature-toggle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Estes",
            "email": "joshua@sonsofphp.com"
        }
    ],
    "support": {
        "issues": "https://github.com/SonsOfPHP/sonsofphp/issues",
        "forum": "https://github.com/orgs/SonsOfPHP/discussions"
    },
    "autoload": {
        "psr-4": {
            "SonsOfPHP\\Component\\FeatureToggle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4"
    },
    "extra": {
        "sort-packages": true,
        "branch-alias": {
            "dev-main": "0.2.x-dev"
        }
    }
}